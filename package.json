{
  "name": "cqrs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": false,
    "modulePaths": [
      "."
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/?(*.)(test).(js|ts)?(x)"
    ],
    "testPathIgnorePatterns": [
      "/build/"
    ]
  },
  "dependencies": {
    "@eventuateinc/eventuate-nodejs-client": "^1.0.25",
    "@types/jest": "^23.3.10",
    "io-ts": "0.9.4",
    "is-url": "^1.2.4",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "uuid": "^3.3.2",
    "mongoose": "^5.4.0",
    "io-ts-validation": "^1.3.3"
  },
  "devDependencies": {
    "@types/mongoose": "^5.3.5"
  }
}
